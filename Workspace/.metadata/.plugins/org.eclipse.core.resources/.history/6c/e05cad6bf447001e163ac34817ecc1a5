package com.model;

public class Employee {
	private int empId;
	private String empName;
	private int salary;
	private int deptfkId;
	public Employee(int empId, String empName, int salary, int deptfkId) {
		super();
		this.empId = empId;
		this.empName = empName;
		this.salary = salary;
		this.deptfkId = deptfkId;
	}
	public int getEmpId() {
		return empId;
	}
	public void setEmpId(int empId) {
		this.empId = empId;
	}
	public String getEmpName() {
		return empName;
	}
	public void setEmpName(String empName) {
		this.empName = empName;
	}
	public int getSalary() {
		return salary;
	}
	public void setSalary(int salary) {
		this.salary = salary;
	}
	public int getDeptfkId() {
		return deptfkId;
	}
	public void setDeptfkId(int deptfkId) {
		this.deptfkId = deptfkId;
	}
	
}
