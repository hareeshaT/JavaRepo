package Day3;

public class UserMainCode {
	public static String getString(String str) {
		String newStr="";
		for (int i = 0; i < str.length(); i++) {
			if(str.charAt(0)=='k') {
				if(str.charAt(1)=='b') {
					newStr+=str.charAt(1);
				}
			}
			newStr+=str.charAt(0);
			newStr+=str.charAt(i);
		}
		return newStr;
	}
	public static void main(String[] args) {
		String result=getString("hello");
		System.out.println(result);
	}

}
